#!/usr/bin/env mkb
# Builder mkb file for the java portion of the bluetooth extension on android
platform JAVA

files
{
    (source/android)
    bluetooth.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/bluetooth.jar
